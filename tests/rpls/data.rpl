RPL { lib: std }

iostatic File {
	name: [data.rpl, test.data.rpl]
}

format PascalStr {
	endian: little
	sign: unsigned

	xlen: [number, 1]
	xstr: [string, @this.xlen]
}

data TestFile {
	endian: little
	sign: unsigned
	file: @File.name

	xpstrbase: [number, 4]
	# TODO: More tests in header here
	xpstramt: [number, 2, @this.xpstrbase]
	xpstrs: [@PascalStr, @this.xpstramt]
}
